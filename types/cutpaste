default partial xkb_types "addsuper" {
    type "ALPHABETIC_SUPER" {
       modifiers = Shift+Mod4;
       map[None] = Level1;
       map[Shift] = Level2;
       map[Mod4] = Level3;
       level_name[Level1] = "Base";
       level_name[Level2]= "Caps";
       level_name[Level3] = "Super";
    };
};
